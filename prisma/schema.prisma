generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Mahasiswa {
  nim String @id
  nama String
  kelas String 
  jurusan String
  jenis_kelamin jenis_kelamin 
}

enum jenis_kelamin {
  L
  P
}

model User {
  id Int @id @default(autoincrement())
  username String @unique
  password String
  role Role @default(USER)
}
enum Role {
  ADMIN
  USER
}

